<h1 class='heading-1'>explore projects<span class='dot'>.</span></h1>

<ul>
  {{#each projects}}
    <li>{{this.category}}</li>
    <br />
    <li>{{this.title}}</li>
    <br />
    <li>{{this.description}}</li>
    <br />
    {{#each this.skills}}
      <p>{{this}}</p>
    {{/each}}
    <br />
    <img src='{{this.image}}' alt='' />
  {{/each}}
</ul>